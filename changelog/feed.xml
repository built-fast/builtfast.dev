---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <generator uri="https://jekyllrb.com/">Jekyll</generator>
  <link href="{{ '/changelog/feed.xml' | absolute_url }}" rel="self" type="application/atom+xml"/>
  <link href="{{ '/changelog/' | absolute_url }}" rel="alternate" type="text/html"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ '/changelog/feed.xml' | absolute_url }}</id>
  <title type="html">{{ site.title | xml_escape }} - Changelog</title>
  <subtitle>Product updates and changes from {{ site.author.name | xml_escape }}</subtitle>
  <author>
    <name>{{ site.author.name | xml_escape }}</name>
  </author>
  {% assign changelog_entries = site.changelog | sort: 'date' | reverse %}
  {% for entry in changelog_entries limit:20 %}
  <entry>
    <title type="html">{{ entry.title | xml_escape }}</title>
    <link href="{{ entry.url | absolute_url }}" rel="alternate" type="text/html" title="{{ entry.title | xml_escape }}"/>
    <published>{{ entry.date | date_to_xmlschema }}</published>
    <updated>{{ entry.date | date_to_xmlschema }}</updated>
    <id>{{ entry.url | absolute_url }}</id>
    <content type="html" xml:base="{{ entry.url | absolute_url }}"><![CDATA[{{ entry.content }}]]></content>
    {% if entry.tags %}
    {% for tag in entry.tags %}
    <category term="{{ tag | xml_escape }}"/>
    {% endfor %}
    {% endif %}
  </entry>
  {% endfor %}
</feed>
